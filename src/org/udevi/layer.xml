<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Templates">
        <folder name="Scripting">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.udevi.Bundle"/>
            <attr name="position" intvalue="20"/>
            <file name="Fancy.php" url="nbresloc:/org/udevi/fancy.php">
                <attr name="iconBase" stringvalue="org/udevi/logo.png"/>
                <attr name="displayName" bundlevalue="org.udevi.Bundle#Templates/Scripting/FancyPHP"/>
                <attr name="javax.script.ScriptEngine" stringvalue="freemarker"/>
                <attr name="instantiatingIterator" newvalue="org.udevi.FancyWizardIterator"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/org/udevi/fancy.html"/>
                <attr name="templateCategory" stringvalue="PHP"/>
                <attr name="position" intvalue="100"/>
            </file>
        </folder>  
    </folder>
</filesystem>
